{
  "name": "xmzapi",
  "version": "0.0.2",
  "description": "XMZAPI是一个基于JavaScript的SDK，旨在帮助开发者快速接入各种API服务。通过这个SDK，用户可以轻松地完成各种API调用。",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf node_modules coverage build",
    "build": "babel src -d build"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@huaweicloud/huaweicloud-sdk-kms": "^3.1.128",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.27.0",
    "axios": "1.7.4",
    "jaeger-client": "^3.19.0",
    "skywalking-backend-js": "^0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.5",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaomizhoubaobei/MZAPI-JS.git"
  },
  "keywords": [
    "SDK",
    "XMZ",
    "JavaScript",
    "API"
  ],
  "author": {
    "name": "小米粥",
    "email": "mzapi@x.mizhoubaobei.top",
    "url": "http://github.com/xiaomizhoubaobei/MZAPI-JS"
  },
  "contributors": [
    {
      "name": "小米粥",
      "email": "mzapi@x.mizhoubaobei.top"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaomizhoubaobei/MZAPI-JS/issues"
  },
  "homepage": "https://github.com/xiaomizhoubaobei/MZAPI-JS#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "lib": "src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}

{
  "name": "mzapi",
  "version": "0.0.1",
  "description": "MZAPI是一个基于JavaScript的SDK,旨在帮助开发者快速接入各种API服务。通过这个SDK,用户可以轻松地完成各种API调用。",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf node_modules coverage build",
    "build": "tsc && babel src -d build",
    "build:types": "tsc --emitDeclarationOnly",
    "docs": "typedoc src/",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@huaweicloud/huaweicloud-sdk-core": "^3.1.140",
    "@huaweicloud/huaweicloud-sdk-kms": "^3.1.128",
    "axios": "^1.8.3",
    "docdash": "^2.0.2",
    "express": "^4.21.2",
    "jaeger-client": "^3.19.0",
    "json": "^11.0.0",
    "pnpm": "^10.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^4.17.21",
    "@types/jaeger-client": "^3.18.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.25.9",
    "typescript": "^5.4.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaomizhoubaobei/MZAPI-JS.git"
  },
  "keywords": [
    "SDK",
    "XMZ",
    "JavaScript",
    "API"
  ],
  "author": {
    "name": "小米粥",
    "email": "mzapi@x.mizhoubaobei.top",
    "url": "https://github.com/xiaomizhoubaobei/MZAPI-JS"
  },
  "contributors": [
    {
      "name": "小米粥",
      "email": "mzapi@x.mizhoubaobei.top"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaomizhoubaobei/MZAPI-JS/issues"
  },
  "homepage": "https://github.com/xiaomizhoubaobei/MZAPI-JS#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "lib": "src"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "jest": true
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
